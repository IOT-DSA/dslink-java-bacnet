{
  "name": "dslink-java-bacnet",
  "version": "2.0.19",
  "description": "A dslink for bacnet",
  "license": "GNU GPL",
  "main": "bin/dslink-java-bacnet",
  "author": {
    "name": "Daniel Shapiro",
    "email": "danielsspam1272@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-bacnet"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-bacnet/issues"
  },
  "configs": {
  	"name": {
      "type": "string",
      "default": "BACnet"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "bacnet.Main"
    }
  }
}
